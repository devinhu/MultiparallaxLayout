<?xml version="1.0" encoding="utf-8"?>
<it.francescopezzato.android.multiparallaxlayout.MultiparallaxLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:multiparallax="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/list_header_height"/>

    <TextView
        style="@style/Base.TextAppearance.AppCompat.Headline"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/list_header_logo"
        android:background="@drawable/header_logo_background"
        multiparallax:ratioX="0.9"
        android:gravity="center"
        android:text="T"
        android:textColor="@android:color/white"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="20dp"
        />
</it.francescopezzato.android.multiparallaxlayout.MultiparallaxLayout>
